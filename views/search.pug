extends layouts/main 
include mixins/videoblock

block content 
    .search__header 
        h3 Searching for #{searchingfor}
    .search__videos 
        if videos.length === 0
            h5 No Videos Found
        each item in videos 
            +videoBlock({
                title:item.title,
                views:item.views,
                videoFile:item.fileUrl,
                id:item.id
            })